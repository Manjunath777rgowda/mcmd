{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: CLI Command",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/mcmd/cli.py",
      "args": ["exec", "ls","ss"],  // Replace with your command and args
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
